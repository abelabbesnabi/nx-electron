{
  "name": "nx-electron",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "Electron Plugin for Nx",
  "author": "Benny Megidish",
  "scripts": {
    "start": "nx run nx-electron:serve",
    "build": "nx run nx-electron:build",
    "link": "npm link ./dist/packages/nx-electron/",
    "test": "nx run nx-electron:test",
    "test:watch": "nx run nx-electron:test -- --watch",
    "lint": "nx run nx-electron:lint",
    "publish": "cd dist/packages/nx-electron && npm publish",
    "publish:alpha": "cd dist/packages/nx-electron && npm publish --tag alpha",
    "publish:beta": "cd dist/packages/nx-electron && npm publish --tag beta",
    "release": "cd packages/nx-electron && npm run release"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/core": "18.0.3",
    "@angular-devkit/schematics": "18.0.3",
    "@nx/angular": "19.2.3",
    "@nx/devkit": "19.2.3",
    "@nx/eslint": "19.2.3",
    "@nx/eslint-plugin": "19.2.3",
    "@nx/jest": "19.2.3",
    "@nx/js": "19.2.3",
    "@nx/node": "19.2.3",
    "@nx/plugin": "19.2.3",
    "@nx/workspace": "19.2.3",
    "@schematics/angular": "18.0.3",
    "@swc-node/register": "1.9.2",
    "@swc/core": "^1.5.29",
    "@types/jest": "29.5.12",
    "@types/node": "^20.14.2",
    "@types/webpack": "^5.28.5",
    "@types/webpack-node-externals": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "bufferutil": "^4.0.8",
    "electron": "^31.0.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "nx": "19.2.3",
    "prettier": "3.2.5",
    "shx": "^0.3.4",
    "standard-version": "^9.5.0",
    "ts-jest": "29.1.2",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "rxjs": "^7.8.1"
  },
  "engines": {
    "node": ">= 16.0.0"
  }
}
